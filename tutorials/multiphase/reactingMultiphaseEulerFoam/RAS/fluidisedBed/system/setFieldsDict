FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha.air 1
    volScalarFieldValue alpha.particles 0
    volScalarFieldValue alpha.particles1 0
    volScalarFieldValue alpha.particles2 0
);

regions
(
    boxToCell
    {
        box (0 0 -0.1) (0.15 0.04 0.1);
        fieldValues
        (
            volScalarFieldValue alpha.air 0.37
            volScalarFieldValue alpha.particles     0.27
            volScalarFieldValue alpha.particles1    0.28
            volScalarFieldValue alpha.particles2    0.175
        );
    }
);


